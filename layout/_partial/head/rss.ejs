<%
  const themeRss = theme.url && theme.url.rss
  const feedPath = config.feed && config.feed.path
  const rssUrl = themeRss || (feedPath ? url_for(feedPath) : null)
  const feedType = config.feed && config.feed.type
%>
<% if (rssUrl) { %>
    <% if (feedType === "atom") { %>
        <link rel="alternate" type="application/atom+xml" href="<%= rssUrl %>">
    <% } else if (feedType === "rss2") { %>
        <link rel="alternate" type="application/rss+xml" href="<%= rssUrl %>">
    <% } %>
<% } %>
